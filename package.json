{
  "name": "ts-codemod",
  "bin": "bin/cli",
  "version": "0.0.0-development",
  "description": "Code-Modifier for Typescript based projects",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require=ts-node/register test/*.ts",
    "lint": "tslint --project .",
    "prepublishOnly": "tsc -d",
    "prettier": "git ls-files | grep -E '.*\\.(ts|md|json)$' | xargs prettier --write --config=.prettierrc",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once --pro"
  },
  "author": "Tushar Mathur <tusharmath@gmail.com>",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "large",
    "typescript",
    "code",
    "mod",
    "modifiers",
    "project",
    "scale"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/ts-codemod.git"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^8.0.0",
    "@types/json5": "^0.0.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.12",
    "@types/ramda": "^0.26.29",
    "@types/yargs": "^13.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "mocha": "^6.2.1",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.24",
    "travis-deploy-once": "^5.0.11",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "fs-extra": "^8.1.0",
    "json5": "^2.1.1",
    "ramda": "^0.26.1",
    "ts-curry": "^1.0.4",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "yargs": "^14.2.0"
  }
}
